{
    "env" : {
        "browser" : true,
        "node" : true,
        "es6" : true
    },
    "plugins": [
        "react"
    ],
    "parserOptions": {
        "sourceType": "module",
        "ecmaVersion": 6,
        "ecmaFeatures": {
	        "modules": true,
	        "jsx": true
    	}
    },
    "rules" : {
        "no-unused-vars": 2,
        "no-undef": 2,
        "react/jsx-uses-react": 2,
        "react/jsx-uses-vars": 2,
        "react/react-in-jsx-scope": 2
    }    
}